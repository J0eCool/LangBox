fn foo a b {
    print "foo: " a " " b
    return (+ a b)
}

fn fib n {
    if (< n 2) {
        return n
    }
    return (+ (fib (- n 1)) (fib (- n 2)))
}

print "oh man"
print (foo 2 3)
print (foo 4 5)

print "fib 1: " (fib 1)
print "fib 2: " (fib 2)
print "fib 3: " (fib 3)
print "fib 4: " (fib 4)
print "fib 5: " (fib 5)
print "fib 6: " (fib 6)
print "fib 7: " (fib 7)
